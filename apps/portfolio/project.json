{
  "name": "portfolio",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/portfolio/src",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/portfolio && pnpm dev"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/portfolio && pnpm build"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd apps/portfolio && pnpm preview"
      }
    }
  },
  "tags": ["type:app"]
}